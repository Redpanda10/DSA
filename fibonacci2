#include <iostream> 
using namespace std; 
int fibonacci_numbers(int n) 
{ 
	if(n <=1)
		return 1; 
	else
		return fibonacci_numbers(n-1) + fibonacci_numbers(n-2); 
} 

int main() {
    int x;
	cout<<"Enter the n-terms to print"<<endl;
	cin>>x;
	for(int i = 0; i < x; i++) 
	{ 
		cout << fibonacci_numbers(i) << " "; 
	} 
	return 0; 
} 
